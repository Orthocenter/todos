<section id="todoapp">
    <header id="header">
        <h2>todos</h2>
    </header>

    <section id="main">
        <div id="main-header">
            <input id="new-todo" type="text" placeholder="What needs to be done?" />
        </div>
        <ul id="todo-list">
          {{outlet}}
        </ul>
    </section>

    <section id="footer">
        <span id="todo-count">
          <strong>2</strong> items left
        </span>
        <ul id="filter">
            <li class="active">All</li>
            <li>Active</li>
            <li>Completed</li>
        </ul>
    </section>
</section>